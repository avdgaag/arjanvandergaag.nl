---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>Arjan van der Gaag</title>
    <link href="http://arjanvandergaag.nl/atom.xml" rel="self"/>
    <link href="http://arjanvandergaag.nl/"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://arjanvandergaag.nl/</id>
    <author>
        <name>Arjan van der Gaag</name>
        <email>arjan@arjanvandergaag.nl</email>
    </author>

    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://arjanvandergaag.nl{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://arjanvandergaag.nl{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}

</feed>